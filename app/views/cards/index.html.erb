<div class="container">

    <h1 class="main-h1">Klanz Deckbuilder</h1>
    <div class="row">
      <div class="col-md-2">
      </div>
      <div class="col-md-10">
        <table class="table table-striped">
          <tbody id="cards">
            <% render @cards %>
          </tbody>
        </table>
        <div id="paginator">
          <%= paginate @cards, :remote => true %>
        </div>
      </div>
    </div>

</div>