<thead>
<tr>
  <th>Имя деки</th>
  <th>Кланы</th>
  <th>Формат</th>
  <th>Карты</th>
  <th>Стоимость</th>
  <th>Автор</th>
</tr>
</thead>
<tbody>
<% @decks.each do |deck| %>
    <tr class="deck-row" data-link="<%= deck_path(deck.id) %>">
      <td><%= deck.name %></td>
      <td>
        <% get_clans(deck).each do |clan| %>
            <%= image_tag "filter_icons/#{clan_id(clan)}.png" %>
        <% end %>
      </td>
      <td>
        <%= deck.room %>
      </td>
      <td>
        <%= get_card_list(deck.cards) %>
      </td>
      <td>
        <%= image_tag "modules.png", class: 'price' %><%= deck.module_price %><br>
        <%= image_tag "stars.png", class: 'price' %><%= deck.star_price %>
      </td>
      <td><%= deck.user.username %></td>
    </tr>
<% end %>
</tbody>
